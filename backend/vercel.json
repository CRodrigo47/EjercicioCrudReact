{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ],
    "env": {
      "mongo_uri": "@mongo_uri"
    }
  }
  